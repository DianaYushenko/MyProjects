#include <iostream>
using namespace std;
void main()
{
	setlocale(0, "Ukrainian");
	int a, b, c;
	cout << "Введіть три числа \n\n";
	cin >> a >> b >> c;
	if (a < b && a < c) 
		{
			cout << "Перше число найменше. \n\n";
		}
		else if (b < a && b < c)
		{
			cout << "Друге число найменше. \n\n";
		}
		else
		{
			cout << "Третє число найменше.\n\n";
		}
}
