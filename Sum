#include <iostream>
using namespace std;
int main() {
	setlocale (0,"Ukrainian");
	int min, max, sum=0;
	cout << "Введіть найменше число з діапазону: ";
	cin >> min;
	cout << "Введіть найбільше число з діапазону: ";
	cin >> max;
	for (int i = min; i <= max; i++) {
		sum += i;
	}
	cout << "Сума чисел в цьому діапазоні дорівнює: " << sum << ".\n";
}
